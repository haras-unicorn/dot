{
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "workbench.colorTheme": "Community Material Theme Palenight High Contrast",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Regular', 'monospace'",
  "terminal.integrated.fontSize": 12,
  "editor.inlayHints.fontFamily": "'JetBrainsMono Nerd Font Regular', 'monospace'",
  "debug.console.fontFamily": "'JetBrainsMono Nerd Font Regular', 'monospace'",
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font Regular', 'monospace'",
  "editor.inlayHints.fontSize": 12,
  "debug.console.fontSize": 12,
  "editor.codeLensFontFamily": "'JetBrainsMono Nerd Font Regular', 'monospace'",
  "editor.codeLensFontSize": 10,
  "window.zoomLevel": 2,
  "editor.rulers": [81],
  "scm.inputFontSize": 12,
  "terminal.integrated.defaultProfile.linux": "fish",
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "explorer.confirmDragAndDrop": false,
  "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init.lua",
  "terminal.external.linuxExec": "kitty",
  "terminal.explorerKind": "external",
  "editor.fontLigatures": true,
  "gulp.autoDetect": "on",
  "git.enabled": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "Lua.runtime.version": "LuaJIT",
  "Lua.diagnostics.globals": ["vim"],
  "todohighlight.keywords": ["TODO", "FIXME", "BUG", "NOTE", "FIX"],
  "workbench.panel.defaultLocation": "right",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "omnisharp.analyzeOpenDocumentsOnly": true,
  "omnisharp.enableDecompilationSupport": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.organizeImportsOnFormat": true,
  "omnisharp.waitForDebugger": true
}
