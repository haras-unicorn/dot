{
  "[aspnetcorerazor]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "debug.console.fontFamily": "\"JetBrainsMono Nerd Font\"",
  "debug.console.fontSize": 13,
  "debug.focusWindowOnBreak": false,
  "debug.onTaskErrors": "abort",
  "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
  "debug.terminal.clearBeforeReusing": true,
  "dotnet-test-explorer.enableTelemetry": false,
  "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "\"JetBrainsMono Nerd Font\"",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "off",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    {
      "color": "#CCCCDD",
      "column": 80
    }
  ],
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoCheckUpdates": false,
  "files.autoSave": "onFocusChange",
  "files.enableTrash": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "git.enabled": false,
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "yaml": true
  },
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "omnisharp.enableAsyncCompletion": true,
  "omnisharp.useEditorFormattingSettings": true,
  "omnisharp.useModernNet": true,
  "powershell.promptToUpdatePowerShell": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.configFile": "~/AppData/Roaming/code_ssh_config",
  "search.followSymlinks": false,
  "search.useIgnoreFiles": false,
  "security.workspace.trust.untrustedFiles": "open",
  "task.saveBeforeRun": "always",
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "/nix/store/c1d0bgq6whz4khqxncmqikpdsxmr1szw-kitty-0.32.2/bin/kitty",
  "terminal.integrated.automationProfile.linux": {
    "path": "/nix/store/d4d4r674rzyf2iznb3f2zclld205ci17-bash-interactive-5.2p26/bin/bash"
  },
  "terminal.integrated.defaultProfile.linux": "nushell",
  "terminal.integrated.fontFamily": "\"JetBrainsMono Nerd Font\"",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "/nix/store/d4d4r674rzyf2iznb3f2zclld205ci17-bash-interactive-5.2p26/bin/bash"
    },
    "nushell": {
      "path": "/nix/store/3x7rkfqh6z40p9zgv8whbjd9czmbd2z4-nushell-0.91.0/bin/nu"
    }
  },
  "testing.saveBeforeTest": true,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "none",
  "vim.commandLineModeKeyBindingsNonRecursive": [
    {
      "before": ["dbg"],
      "commands": ["workbench.action.debug.start"]
    }
  ],
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>", "workbench.action.files.save"],
      "before": ["j", "k"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "s", "d"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "s", "l"],
      "commands": ["workbench.action.splitEditorLeft"]
    },
    {
      "before": ["<leader>", "s", "u"],
      "commands": ["workbench.action.splitEditorUp"]
    },
    {
      "before": ["<leader>", "s", "r"],
      "commands": ["workbench.action.splitEditorRight"]
    }
  ],
  "vim.sneak": true,
  "vim.textwidth": 80,
  "vim.useSystemClipboard": true,
  "window.commandCenter": false,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "debugConsole.errorForeground": "#ff0000",
    "debugConsole.infoForeground": "#00ff00",
    "debugConsole.sourceForeground": "#ffffff",
    "debugConsole.warningForeground": "#ffff00"
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "readme"
}
